<div class="panel center">
  <div class="col s12 m6 l12">
    <h4>Mon emploi du temps sur
      <%= @reservation.number_of_weeks %>
      semaines
      <a class="btn-floating btn green">
        <i class="material-icons">mode_edit</i>
      </a>
    </h4>
    <div class="row">
      <div class="col s12 m3 l2 offset-l1 offset-m1">
        <div class="">
          <h4 class="center">Lundi</h4>
        </div>
        <% if @week.worker_monday_morning %>
        <div class="card-panel center z-depth-2 card green lighten-2">
          Matin
        </div>
      <% else %>
        <div class="card-panel center z-depth-2">
          Matin
        </div>
        <% end %>
        <% if @week.worker_monday_afternoon %>
        <div class="card-panel center z-depth-2 card green lighten-2">
          Après-midi
        </div>
      <% else %>
        <div class="card-panel center z-depth-2">
          Après-midi
        </div>
        <% end %>
      </div>

      <div class="col s12 m3 l2">
        <div class="">
          <h4 class="center">Mardi</h4>
        </div>
        <% if @week.worker_tuesday_morning %>
        <div class="card-panel center z-depth-2 card green lighten-2">
          Matin
        </div>
      <% else %>
        <div class="card-panel center z-depth-2">
          Matin
        </div>
        <% end %>
        <% if @week.worker_tuesday_afternoon %>
        <div class="card-panel center z-depth-2 card green lighten-2">
          Après-midi
        </div>
      <% else %>
        <div class="card-panel center z-depth-2">
          Après-midi
        </div>
        <% end %>
      </div>

      <div class="col s12 m3 l2">
        <div class="">
          <h4 class="center">Mercredi</h4>
        </div>
        <% if @week.worker_wednesday_morning %>
        <div class="card-panel center z-depth-2 card green lighten-2">
          Matin
        </div>
      <% else %>
        <div class="card-panel center z-depth-2">
          Matin
        </div>
        <% end %>
        <% if @week.worker_wednesday_afternoon %>
